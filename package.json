{
  "name": "@slimio/eslint-config",
  "version": "2.0.4",
  "description": "Slimio ESLint configuration",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "node test/test.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/eslint-config.git"
  },
  "keywords": [
    "ESLint",
    "Config"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/eslint-config/issues"
  },
  "homepage": "https://github.com/SlimIO/eslint-config#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "eslint": "^5.16.0",
    "husky": "^2.0.0",
    "pkg-ok": "^2.3.1"
  },
  "peerDependencies": {
    "eslint": ">= 5"
  }
}
